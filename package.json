    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "laravel-vite-plugin": "^0.7.5",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "vite": "^4.0.0",
    "vue": "^3.4.15",
    "vuetify": "^3.3.12"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
